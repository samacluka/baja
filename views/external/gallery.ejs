<% const views = '../' %>
<%- include(views+'partials/header4'); %>

<div class="container">
  <div class="p-3">
    <div class="row text-center" style="display: flex; flex-wrap: wrap;">
      <div class="row row-eq-height">
        <% images.resources.forEach(function(image){ %>
           <div class="col-lg-4 col-md-6 col-sm-6">
                <div class="card card-link-floater card-shadow">
                    <div class="card-img-mask card-img-mask-top">
                        <img src=<%= image.url %> alt="image">
                    </div>
                    <div class="card-body">
                         <h3 class="card-title"><%= image.filename %></h3>
                    </div>
                </div>
            </div>
          <% }); %>
      </div>
    </div>
  </div>
</div>

<%- include(views+'partials/footer4'); %>
